    <script>
        function cancelOrder(orderId, productId) {
            Swal.fire({
                title: 'Are you sure?',
                text: "You won't be able to revert this!",
                // icon: 'warning',
                icon: 'question',
                iconHtml: '',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes, Cancel Order!'
            }).then(async (result) => {
                if (result.value) {
                    const { value: text } = await Swal.fire({
                        input: 'textarea',
                        inputLabel: 'Message',
                        inputPlaceholder: 'Type your message here...',
                        inputAttributes: {
                            'aria-label': 'Type your message here'
                        },
                        showCancelButton: true
                    })
                    if (text) {
                        let reason = text;
                        $.ajax({
                            url: `/user/cancel-order/?orderId=${orderId}&productId=${productId}&reason=${encodeURIComponent(reason)}`,
                            method: "GET",
                            success: (response) => {
                                if (response.status) {
                                    Swal.fire(
                                        'Cancelled!',
                                        'Your Order Cancelled.',
                                        'success'
                                    ).then(() => {
                                        location.reload(); // reload the current page
                                    })
                                }
                            }
                        })
                    }
                }
            }).catch(error => {
                console.error(error);
                Swal.fire('Error!', 'An error occurred while deleting the subcategory.', 'error');
            });
        }

        function returnOrder(orderId, productId) {
            Swal.fire({
                title: 'Are you sure?',
                text: "You won't be able to revert this!",
                // icon: 'warning',
                icon: 'question',
                iconHtml: '',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes, Return Order!'
            }).then(async (result) => {
                if (result.value) {
                    const { value: text } = await Swal.fire({
                        input: 'textarea',
                        inputLabel: 'Message',
                        inputPlaceholder: 'Type your message here...',
                        inputAttributes: {
                            'aria-label': 'Type your message here'
                        },
                        showCancelButton: true
                    })

                    if (text) {
                        let reason = text;
                        $.ajax({
                            url: `/user/return-order/?orderId=${orderId}&productId=${productId}&reason=${encodeURIComponent(reason)}`,
                            method: "GET",
                            success: (response) => {
                                if (response.status) {
                                    Swal.fire(
                                        'Done!',
                                        'Your Order Returned.',
                                        'success'
                                    ).then(() => {
                                        location.reload(); // reload the current page
                                    })
                                } else {
                                    Swal.fire({
                                        position: 'top-end',
                                        icon: 'info',
                                        title: 'Your Return has Expired',
                                        showConfirmButton: false,
                                        timer: 1500
                                    })
                                }
                            }
                        })
                    }
                }
            }).catch(error => {
                console.error(error);
                Swal.fire('Error!', 'An error occurred !.', 'error');
            });

        }
    </script>




